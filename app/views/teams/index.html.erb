<p id="notice"><%= notice %></p>

<h1>Teams</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Lg</th>
      <th>Team</th>
      <th>Franch</th>
      <th>Div</th>
      <th>Rank</th>
      <th>Ghome</th>
      <th>Div win</th>
      <th>Wc win</th>
      <th>Lg win</th>
      <th>Ws wing</th>
      <th>Name</th>
      <th>Park</th>
      <th>Attendance</th>
      <th>Bpf</th>
      <th>Ppf</th>
      <th>Team idrb</th>
      <th>Team i dlahman45</th>
      <th>Team i dretro</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teams.each do |team| %>
      <tr>
        <td><%= team.year_id %></td>
        <td><%= team.lg_id %></td>
        <td><%= team.team_id %></td>
        <td><%= team.franch_id %></td>
        <td><%= team.div_id %></td>
        <td><%= team.rank %></td>
        <td><%= team.ghome %></td>
        <td><%= team.div_win %></td>
        <td><%= team.wc_win %></td>
        <td><%= team.lg_win %></td>
        <td><%= team.ws_wing %></td>
        <td><%= team.name %></td>
        <td><%= team.park %></td>
        <td><%= team.attendance %></td>
        <td><%= team.bpf %></td>
        <td><%= team.ppf %></td>
        <td><%= team.team_idrb %></td>
        <td><%= team.team_i_dlahman45 %></td>
        <td><%= team.team_i_dretro %></td>
        <td><%= link_to 'Show', team %></td>
        <td><%= link_to 'Edit', edit_team_path(team) %></td>
        <td><%= link_to 'Destroy', team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Team', new_team_path %>
